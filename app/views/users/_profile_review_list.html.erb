  <div class="container ">
    <div class="row">
      <div class="col-xs-4 col-xs-offset-4">

        <% @reviews.each do |review|  %>
          <p><%= review.content %></p>
          <p><%= review.rating %></p>
          <p><%= review.booking.event.date %></p>
          <p><%= review.booking.event.title %></p>
          <p><%= review.booking.event.location %></p>
          <p><%= review.booking.event.user.first_name %></p>
          <p><%= review.booking.event.user.photo %></p>
        <%end %>

      </div>
    </div>
  </div>
